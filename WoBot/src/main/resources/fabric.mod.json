{
  "schemaVersion": 1,
  "id": "wobot-addon",
  "version": "${version}",
  "name": "WoBot Addon",
  "description": "WoBot addon for Meteor Client.",
  "authors": [
    "WoBo"
  ],
  "contact": {
    "repo": "https://github.com/Billyc27/WoBot"
  },
  "icon": "assets/WoBot/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "WoBot.WOBOT"
    ]
  },
  "mixins": [
    "WoBot.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "5,5,5"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*"
  }
}
